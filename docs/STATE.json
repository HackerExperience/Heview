{
  "game": {
    "account": {
      
    },
    "servers": {
      "$cid": {
        "logs": {
          "entries": {
            "$id": {
              "timestamp": "timestamp",
              "type": "Log.type",
              "data": "map :: Log.data"
            }
          },
          "sorted (derived)": ["Log.id"]
        }
      },
      "meta": {
        "player": [
          {
            "type": "string",
            "server_id": "Server.id",
            "nips": ["NIP.t"],
            "endpoints": ["NIP.t"]
          }
        ]
      }
    },
    "story": {}
  },

  "web": {
    "wm": {
      "sessions": {
        "$cid": {
          "gateway": "cid",
          "endpoint": "cid | nil",
          "apps": ["$app-id"]
        }
      },
      "windows": {
        "$app-id": {
          "position": {
            "x": "int",
            "y": "int",
            "len_x": "int",
            "len_y": "int"
          }
        }
      },
      "active-session": "$cid"
    },

    "apps": {
      "$app-id": {
        "state": {
          "current": {},
          "other": {}
        },
        "meta": {
          "context": ":local | :remote",
          "type": "$app-type",
          "session": "$cid",
          "active-context (app)": "$cid"
        }
      }
    }
  }
}
